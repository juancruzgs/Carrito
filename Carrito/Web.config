<configuration>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />

        <authentication mode="Forms">          
          <forms defaultUrl="FormPrincipal.aspx" loginUrl="FormLogin.aspx"></forms>        
        </authentication>
    </system.web>
  
    <system.webServer>
      <defaultDocument>
        <files>
          <clear />
          <add value="FormPrincipal.aspx" />
        </files>
      </defaultDocument>
    </system.webServer>

</configuration>
